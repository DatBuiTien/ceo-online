<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="ceo_reset_pass_token_tree" model="ir.ui.view">
        <field name="name">opencourse.reset_pass_token.tree</field>
        <field name="model">opencourse.reset_pass_token</field>
        <field name="arch" type="xml">
            <tree>
                <field name="login"/>
                <field name="email"/>
                <field name="code"/>
                <field name="date_expire"/>
                <field name="reset_link"/>
            </tree>
        </field>
    </record>

    <record id="action_ceo_online_reset_pass_token" model="ir.actions.act_window">
        <field name="name">opencourse.reset_pass_token</field>
        <field name="res_model">opencourse.reset_pass_token</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree,form</field>
    </record>
</odoo>