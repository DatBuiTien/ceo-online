<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="ceo_permission_tree" model="ir.ui.view">
        <field name="name">opencourse.permission.tree</field>
        <field name="model">opencourse.permission</field>
        <field name="arch" type="xml">
            <tree string="Permission">
                <field name="name"/>
                <field name="menu_access"/>
                <field name="user_group_id"/>
            </tree>
        </field>
    </record>

    <record id="action_ceo_online_permission" model="ir.actions.act_window">
        <field name="name">Permission</field>
        <field name="res_model">opencourse.permission</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree,form</field>
    </record>
</odoo>