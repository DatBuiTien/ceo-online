<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="ceo_accout_form" model="ir.ui.view">
        <field name="name">account.form</field>
        <field name="model">account.account</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="code"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="ceo_account_tree" model="ir.ui.view">
        <field name="name">account.tree</field>
        <field name="model">account.account</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="code"/>
            </tree>
        </field>
    </record>

    <record id="action_ceo_online_account" model="ir.actions.act_window">
        <field name="name">account</field>
        <field name="res_model">account.account</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree,form</field>
    </record>
</odoo>